doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title See Files
        link(rel="stylesheet", href="/css/homeCards.css")
    body
        .container
            each dir, index in dirs
                .card
                    .box
                        .content 
                            h2=index + 1
                            h3=dir
                            a(href=`/see?dir=${dir}`) See Files
